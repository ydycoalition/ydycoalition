%header.ae-container-fluid.ae-container-fluid--full.rk-header
  %input#mobile-menu.rk-mobile-menu{:type => "checkbox"}/
  %label{:for => "mobile-menu"}
    %svg
      %use{"xlink:href" => "assets/images/symbols.svg#bar"}
    %svg
      %use{"xlink:href" => "assets/images/symbols.svg#bar"}
    %svg
      %use{"xlink:href" => "assets/images/symbols.svg#bar"}
  .ae-container-fluid.rk-topbar
    %h1.rk-logo
      = link_to "DO Coalition", root_path
    %nav.rk-navigation
      %ul.rk-menu
        %li.active.rk-menu__item
          = link_to "Home", root_path, class: 'rk-menu__link'
        %li.rk-menu__item
          = link_to "Actions", 
          %a.rk-menu__link{:href => "portfolio.html"} Actions
        %li.rk-menu__item
          %a.rk-menu__link{:href => "blog.html"} Blog
        %li.rk-menu__item
          %a.rk-menu__link{:href => "#0"} Pages
          %nav.rk-menu__sub
            %ul.rk-container
              %li.rk-menu__item
                %a.rk-menu__link{:href => "about.html"} About
              %li.rk-menu__item
                %a.rk-menu__link{:href => "documentation.html"} Documentation
              %li.rk-menu__item
                %a.rk-menu__link{:href => "design-styles.html"} Design Styles
        %li.rk-menu__item
          %a.rk-menu__link{:href => "contact.html"} Contact Us
      %form.rk-search
        %input#urku-search.rk-search-field{:placeholder => "Search", :type => "text"}/
        %label{:for => "urku-search"}
          %svg
            %use{"xlink:href" => "assets/img/symbols.svg#icon-search"}
%section.ae-container-fluid.rk-main
  %input#layout-base.layout-ctrl-input{:checked => "checked", :name => "layout-ctrl", :type => "radio"}/
  .ae-container-fluid.rk-layout-ctrl-cont
    %label.rk-layout-ctrl{:for => "layout-base"}
      %svg
        %use{"xlink:href" => "assets/img/symbols.svg#icon-view-default"}
  %input#layout-grid.layout-ctrl-input{:name => "layout-ctrl", :type => "radio"}/
  .ae-container-fluid.rk-layout-ctrl-cont
    %label.rk-layout-ctrl{:for => "layout-grid"}
      %svg
        %use{"xlink:href" => "assets/img/symbols.svg#icon-view-alt"}
  .rk-layout-ctrl-mobile
    %svg.layout-mob-1{:viewbox => "0 0 9 9"}
      %rect{:fill => "currentColor", :height => "100%", :width => "100%"}
    %svg.layout-mob-2{:viewbox => "0 0 9 9"}
      %rect{:fill => "currentColor", :height => "100%", :width => "100%"}
    %svg.layout-mob-3{:viewbox => "0 0 9 9"}
      %rect{:fill => "currentColor", :height => "100%", :width => "100%"}
      %svg.layout-mob-4{:viewbox => "0 0 9 9"}
        %rect{:fill => "currentColor", :height => "100%", :width => "100%"}
    %section.ae-container-fluid.ae-container-fluid--inner.loading.rk-portfolio
      .rk-portfolio__items
        %a.rk-item.rk-item--flex.item-1.rk-size-12.rk-tosize-6.rk-landscape{:href => "portfolio-item.html"}
