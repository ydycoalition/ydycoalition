          %a.rk-item.rk-item--flex.item-11.rk-size-12.rk-tosize-6.rk-landscape.rk-tosquare{:href => "portfolio-item.html"}
            .item-meta
              %h2 Deep v kogi
              %p Graphic Design
          %a.rk-item.rk-item--flex.item-12.rk-size-12.rk-tosize-6.rk-landscape.rk-tosquare{:href => "portfolio-item.html"}
            .item-meta
