              %h2 Biodiesel etsy
              %p Branding
        %a.rk-item.rk-item--flex.item-6.rk-size-6.rk-tosize-4.rk-portrait.rk-tosquare{:href => "portfolio-item.html"}
          .item-meta
            %h2 Gentrify artisan
            %p Graphic Design
        .rk-items-cont.rk-size-4.rk-tosize-8
          %a.rk-item.rk-item--flex.item-7.rk-size-12.rk-tosize-6.rk-square{:href => "portfolio-item.html"}
